{
  "root": true,
  "extends": ["next/core-web-vitals"],
  "ignorePatterns": [
    ".next",
    "node_modules",
    "public",
    "out",
    "dist"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "@next/next/no-img-element": "off",
    "react/react-in-jsx-scope": "off"
  }
}
